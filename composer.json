{
    "name": "maxsky/cn-esign-client",
    "description": "CN ESign PHP Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MaxSky\\ESign\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "ESignClientTests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.8.0",
        "nesbot/carbon": "^2.70.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0|^8.0|^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
